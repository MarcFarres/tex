
  <div id="datas_list_contenedor" class="maquina_list_tests">
        <h2> Resultats </h2>

       <table id="tests_table" class="stripe data_tests_table" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th> Hora </th>
          <th> OF </th>
          <th> Color </th>
          <th> Linia </th>
          <th> Familia </th>
          <th> Maquina </th>
          <th> X </th>
          <th class='obtingut'> X </th>
          <th> DESC% </th>
          <th class='obtingut'> DESC% </th>
          <th> DEV% </th>
          <th class='obtingut'> DEV% </th>
          
        </tr>
      </thead>

      <tbody>

  {% for resultat in tests_list %}

  {% set test = resultat.test %}
  {% set maquina = resultat.maquina %}
  {% set OF = test.Of %}

  <tr class='unclicked clickedException'>

    <td>{{ resultat.data | date('H:i:s')  }}</td>
    <td><a href="{{ path('iniciar_test',{'OF':OF.id}) }}">{{ OF.numero }}</a> </td>
    <td>{{ OF.color }}</td>
    <td>{{ OF.getLinia().numero }}</td>
    <td>{{  maquina.familia.nom  }}</td>
    <td>{{maquina.numero}}</td>
    <td>{{ resultat.mitjanaesp }}</td>
    <td>{{ resultat.mitjana|number_format(2, '.', ',')}}
    <td>{{ resultat.descmax }} %</td>
    <td class='{{ resultat.descentratge > resultat.descmax ? 'error' : '' }}'>{{ resultat.descentratge|number_format(2, '.', ',') }} %</td>
    <td>{{ resultat.devmax }} %</td>
    <td class='{{ resultat.descentratge > resultat.descmax ? 'error' : '' }}'>{{ resultat.dev|number_format(2, '.', ',') }} %</td>
    
    </tr>


      {% endfor %}

    </tbody>
  </table>
      
    </div>


   





